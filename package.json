{
    "name": "swc-registry",
    "version": "2.0.5",
    "description": "Javascript library for accessing SWC-registry content.",
    "repository": "SmartContractSecurity/SWC-registry-javascript",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "bin": {
        "swc-cli": "lib/cli.js"
    },
    "keywords": [
        "swcregistry",
        "npm",
        "package"
    ],
    "files": [
        "lib/cli.js",
        "lib/index.js",
        "lib/index.d.ts",
        "lib/swc-definition.json"
    ],
    "devDependencies": {
        "@types/jest": "^24.0.21",
        "@types/node": "^10.17.2",
        "jest": "^24.9.0",
        "prettier": "^1.18.2",
        "ts-jest": "^24.1.0",
        "tslint": "^5.20.0",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.6.4"
    },
    "dependencies": {
        "@types/es6-promise": "^3.3.0",
        "@types/node-fetch": "^2.5.2",
        "es6-promise": "^4.2.8",
        "isomorphic-fetch": "^2.2.1",
        "load-json-file": "^5.3.0",
        "ts-mockito": "^2.5.0",
        "xmlhttprequest-ts": "^1.0.1"
    },
    "scripts": {
        "clean": "rm -rf lib/ && rm -rf node_modules/",
        "build": "tsc && cp src/swc-definition.json lib/ && cp README.md lib/ && chmod +x lib/cli.js",
        "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
        "swc:update": "lib/cli.js --update && cp lib/swc-definition.json src/",
        "test": "jest",
        "prepare": "npm run build",
        "prepublishOnly": "npm test"
    },
    "author": "ersul4ik",
    "license": "MIT"
}
